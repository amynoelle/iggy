<?xml version="1.0"?>
<launch>

<param name="pub_images" type="bool" value="true"/>


    <!-- TRANSFORMS  -->
    <node pkg="tf" type="static_transform_publisher" name="zed_to_map"
	    args="0.44 0.0 1.18 0 0.52 0 map zed_current_frame 100"/>

    #### STEREO VISION ######################################################    
    <machine name="igvc-stereo" address="igvc-stereo" user="ubuntu" password="ubuntu"
	    env-loader="/home/ubuntu/catkin_ws/devel/env.sh"
	    default="false" />

   <!--  <node pkg="igvc_stereo" type="whitevision2" output="screen" name="whitevision" machine="igvc-stereo"/>

    Start RVIZ -->
    <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find iggy_bringup)/launch/config/vision_only.rviz"/>

<!-- <node pkg="rqt_reconfigure" type="rqt_reconfigure" name="rqt_reconfigure"/>      -->

</launch>
